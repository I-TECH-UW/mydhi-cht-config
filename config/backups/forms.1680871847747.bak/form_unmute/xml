      <bind nodeset="/unmute/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/unmute/inputs/contact/name" type="string"/>
      <bind nodeset="/unmute/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/unmute/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/unmute/inputs/contact/sex" type="string"/>
      <bind nodeset="/unmute/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/unmute/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/unmute/source" type="string" calculate="../inputs/source"/>
      <bind nodeset="/unmute/source_id" type="string" calculate="../inputs/source_id"/>
      <bind nodeset="/unmute/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/unmute/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/unmute/patient_id" type="string" required="true()" calculate="../inputs/contact/_id"/>
      <bind nodeset="/unmute/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/unmute/unmute_request/reason" type="select1" required="true()"/>
      <bind nodeset="/unmute/unmute_request/reason_other" type="string" required="true()" relevant="selected(../reason, 'other')"/>
      <bind nodeset="/unmute/group_review/n_submit" readonly="true()" type="string"/>
      <bind nodeset="/unmute/group_review/n_patient_details_title" readonly="true()" type="string"/>
      <bind nodeset="/unmute/group_review/n_patient_details" readonly="true()" type="string"/>
      <bind nodeset="/unmute/group_review/n_reasons_title" readonly="true()" type="string"/>
      <bind nodeset="/unmute/group_review/n_moved_back" readonly="true()" type="string" relevant="selected(../../unmute_request/reason, 'moved_back')"/>
      <bind nodeset="/unmute/group_review/n_other" readonly="true()" type="string" relevant="selected(../../unmute_request/reason, 'other')"/>
      <bind nodeset="/unmute/group_review/n_special_instruction_title" readonly="true()" type="string"/>
      <bind nodeset="/unmute/group_review/n_special_instruction" readonly="true()" type="string"/>
      <bind nodeset="/unmute/group_review/n_follow_up_title" readonly="true()" type="string"/>
      <bind nodeset="/unmute/group_review/n_follow_up" readonly="true()" type="string"/>
